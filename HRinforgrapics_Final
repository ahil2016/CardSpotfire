<script>
    // source mix start


    var srcMxMap = [
        {'srcmxSocialMedia': {label: 'Social Media', val: ''}}, 
        {'srcmxDirect': {label: 'Direct', val: ''}},
        {'srcmxReferal': {label: 'Employee Referal', val: ''}}, 
        {'srcmxInternal': {label: 'Internal', val: ''}},
        {'srcmxAgency': {label: 'Agency', val: ''}},
        {'srcmxColleag': {label: 'College/Grad', val: ''}},
        {'srcmxTemp': {label: 'Temp', val: ''}}, 
        {'srcmxFormer': {label: 'Former Employee', val: ''}}
    ];

    $("#srcmxSocialMedia").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxSocialMedia') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxDirect").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxDirect') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxReferal").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxReferal') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxInternal").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxInternal') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxAgency").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxAgency') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxColleag").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxColleag') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxTemp").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxTemp') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    $("#srcmxFormer").on('DOMSubtreeModified', function () {
        if (!isNaN(parseInt($(this).text()))) {
            for(var i=0; i< srcMxMap.length; i++) {
                var srcMxEle = srcMxMap[i];
                var srcMxEleKey = Object.keys(srcMxEle)[0];
                if(srcMxEleKey == 'srcmxFormer') {
                    srcMxMap[i][srcMxEleKey]['val'] = parseInt($(this).text());
                    break;
                }
            }
            redrawSrcMx();
        }
    })

    function redrawSrcMx() {
	var isAllValsPresent = true;
	for(var k=0; k<srcMxMap.length; k++) {	
		var srcRawMxItem = srcMxMap[k];
                var srcRawMxItemKey = Object.keys(srcRawMxItem)[0];
                var srcRawMxItemVal = srcRawMxItem[srcRawMxItemKey]['val'];
		if(isNaN(srcRawMxItemVal)) {
			isAllValsPresent = false;
		}
	}

        if(isAllValsPresent) {

            var sortedSrcMx = srcMxMap.sort((a,b) => {
                var aKey = Object.keys(a)[0];
                var bKey = Object.keys(b)[0];
                var aEle = a[aKey];
                var bEle = b[bKey];
                return bEle['val'] - aEle['val']
            })
		var srcMxMaxItem = sortedSrcMx[0];
                var srcMxMaxItemKey = Object.keys(srcMxMaxItem)[0];
                var srcMxMaxItemVal = srcMxMaxItem[srcMxMaxItemKey]['val'];
		
		$('.source-mix-body').html('');
            for(var i=0; i<sortedSrcMx.length; i++) {
                var srcMxItem = sortedSrcMx[i];
                var srcMxItemKey = Object.keys(srcMxItem)[0];
                var srcMxItemVal = srcMxItem[srcMxItemKey]['val'];
                var srcMxItemLabel = srcMxItem[srcMxItemKey]['label'];
                var rowSrxMx = $('<div>', {
                    class: 'source-mix-row'
                }).appendTo('.source-mix-body');

                $('<div>', {
                    class: 'source-mix-label',
                    html: srcMxItemLabel,
                }).appendTo(rowSrxMx);
                
                $('<div>', {
                    class: 'source-mix-axes',
                    html: ' ',
                }).appendTo(rowSrxMx);

                var barContainer = $('<div>', {
                    class: 'source-mix-bar-container tenure-bar-color4',
                    html: ' ',
                    
                }).appendTo(rowSrxMx);
                barContainer.css({'flex-grow': srcMxItemVal/srcMxMaxItemVal});

                $('<div>', {
                    class: 'source-mix-val',
                    html: srcMxItemVal ? srcMxItemVal + '%' : '0%'
                }).appendTo(rowSrxMx);
            }

        }
    }

    // source mix end 
    

    </script>

<script>
	$('.filter-item-head').on('click', function(event) {
		$(event.target).closest('.filter-item-wrapper').toggleClass('open');
		console.log($(event.target).parent());
	})
</script>

