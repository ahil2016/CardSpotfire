    <script>
        $(document).ready(function () {
            $('#hamburger').hover(function () {
                $(this).toggleClass('open');
                $("#menu-container").toggleClass('open');
                $('#menu-container .menu-name').each(function () {
                    $(this).toggleClass('open');
                });
            });
	    $('#menu-container').hover(function () {
                $('#hamburger').toggleClass('open');
                $("#menu-container").toggleClass('open');
                $('#menu-container .menu-name').each(function () {
                    $(this).toggleClass('open');
                });
            });
            $('.filter-icon-container').click(function () {
                $("#filters-container").toggleClass('open');
                $('#filters-container .filter-name').each(function () {
                    $(this).toggleClass('open');
                });
            });
            $('.filters-container').hover(function () {
                $("#filters-container").addClass('open');
                $('#filters-container .filter-name').each(function () {
                    $(this).addClass('open');
                });
            });
	$('.filters-container').mouseleave(function () {
		$("#filters-container").removeClass('open');
                $('#filters-container .filter-name').each(function () {
                    $(this).removeClass('open');
                });
                $('.filter-item-wrapper ').each(function () {
                    $(this).removeClass('open');
                });
            });
        });
    </script>

	<script>
// pyramid head count start
		var grade101Val = '';
		var grade108Val = '';
		var grade111Val = '';
		var grade112Val = '';
		var grade116Val = '';

	$("#grade101 .left-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		grade101Val = parseInt($(this).text());
		redrawPyramid();
	    }
	})
	$("#grade108 .left-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		grade108Val = parseInt($(this).text());
		redrawPyramid();
	    }
	})
	$("#grade111 .left-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		grade111Val = parseInt($(this).text());
		redrawPyramid();	    
	     }
	})
	$("#grade112 .left-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		grade112Val = parseInt($(this).text());
		redrawPyramid();
	    }
	})
	$("#grade116 .left-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		grade116Val = parseInt($(this).text());
		redrawPyramid();
	    }
	})

	function redrawPyramid() {
		if(!isNaN(grade101Val) && !isNaN(grade108Val) && !isNaN(grade111Val) && !isNaN(grade112Val) && !isNaN(grade116Val)

) {

var maxGradVal = Math.max(grade108Val,grade111Val,grade112Val,grade116Val,grade101Val);
	$("#grade101 .pyramid-bar").css("width", ((grade101Val/maxGradVal) * 100) + '%');
$("#grade108 .pyramid-bar").css("width", ((grade108Val/maxGradVal) * 100) + '%');
$("#grade111 .pyramid-bar").css("width", ((grade111Val/maxGradVal) * 100) + '%');
$("#grade112 .pyramid-bar").css("width", ((grade112Val/maxGradVal) * 100) + '%');
$("#grade116 .pyramid-bar").css("width", ((grade116Val/maxGradVal) * 100) + '%');
	
		}

	}
// pyramid head count end
</script>

<script>
// tenure range start
	// tenure range start
		var tenure16Val = '';
		var tenure11Val = '';
		var tenure6Val = '';
		var tenure3Val = '';
		var tenure1Val = '';
		var tenure0Val = '';

	$("#tenure16 .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenure16Val = parseInt($(this).text());
		redrawTenure();
	    }
	})
	$("#tenure11 .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenure11Val = parseInt($(this).text());
		redrawTenure();
	    }
	})
	$("#tenure6 .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenure6Val = parseInt($(this).text());
		redrawTenure();	    
	     }
	})
	$("#tenure3 .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenure3Val = parseInt($(this).text());
		redrawTenure();
	    }
	})
	$("#tenure1 .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenure1Val = parseInt($(this).text());
		redrawTenure();
	    }
	})
	$("#tenure0 .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenure0Val = parseInt($(this).text());
		redrawTenure();
	    }
	})

	function redrawTenure() {

		if(!isNaN(tenure0Val) && !isNaN(tenure16Val) && !isNaN(tenure11Val) && !isNaN(tenure6Val) && !isNaN(tenure3Val) && !isNaN(tenure1Val)

) {

var maxTenureVal = Math.max(tenure0Val,tenure16Val,tenure11Val,tenure6Val, tenure3Val, tenure1Val);

	$("#tenure0 .tenurebar").css("width", ((tenure0Val/maxTenureVal) * 100) + '%');
$("#tenure16 .tenurebar").css("width", ((tenure16Val/maxTenureVal) * 100) + '%');
	$("#tenure11 .tenurebar").css("width", ((tenure11Val/maxTenureVal) * 100) + '%');
$("#tenure6 .tenurebar").css("width", ((tenure6Val/maxTenureVal) * 100) + '%');
$("#tenure3 .tenurebar").css("width", ((tenure3Val/maxTenureVal) * 100) + '%');
$("#tenure1 .tenurebar").css("width", ((tenure1Val/maxTenureVal) * 100) + '%');
	
		}

	}




// tenure range end
</script>




<script>
// diversity start
var tenureDiversityMale = 0;
var tenureDiversityFemale = 0;
	$("#femaleTenureVal .diver-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenureDiversityFemale = parseInt($(this).text());
		redrawTenureDiversity();
	    }
	})
	$("#maleTenureVal .diver-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenureDiversityMale = parseInt($(this).text());
		redrawTenureDiversity();
	    }
	})

function redrawTenureDiversity() {
	
	if(!isNaN(tenureDiversityMale) && !isNaN(tenureDiversityFemale)) {
		$("#femaleTenureVal .diversity-img-bg").css("height", tenureDiversityFemale + '%');
		$("#maleTenureVal .diversity-img-bg").css("height",  tenureDiversityMale + '%');
	}
}


// diversity end

</script>

<script>
// offer acceptancve & Declined start
var acceptance = 0;
var decline = 0;
	$("#acceptance .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		acceptance = parseInt($(this).text());
		redrawAcceptanceRatio();
	    }
	})
	$("#decline .tenureval").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		decline = parseInt($(this).text());
		redrawAcceptanceRatio();
	    }
	})

function redrawAcceptanceRatio(){
	
	if(!isNaN(acceptance) && !isNaN(decline)) {
		$("#acceptance .tenurebar").css("width", acceptance + '%');
		$("#decline .tenurebar").css("width",  decline + '%');
	}
}


// offer acceptancve & Declined start




</script>


