	function redrawAttrPyramid() {
		if(!isNaN(atrGrade101Val) && !isNaN(atrGrade108Val) && !isNaN(atrGrade111Val) && !isNaN(atrGrade112Val) && !isNaN(atrGrade116Val) && !isNaN(opsCwrVal)

) {

var maxAtrGradVal = Math.max(atrGrade108Val,atrGrade111Val,atrGrade112Val,atrGrade116Val,atrGrade101Val);
	$("#atrGrade101 .pyramid-bar").css("width", ((atrGrade101Val/maxAtrGradVal) * 100) + '%');
